net: "ir_gaze_train_val.prototxt"
base_lr: 0.001
lr_policy: "step"
gamma: 0.2
stepsize: 30000
display: 20
max_iter: 40000
momentum: 0.9
weight_decay: 0.0005
## We disable standard caffe solver snapshotting and implement our own snapshot
#snapshot: 0
snapshot: 5000
snapshot_prefix: "GAZE"
#debug_info: true

